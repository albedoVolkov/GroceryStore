<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_frameLayout_account_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityMain.ui.forthTab.AccountFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_constraint_account_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView_1_account_fragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Account"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.060000002" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/inner_data_constaint_account_fragment"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_1_account_fragment">

            <TextView
                android:id="@+id/textView_5_account_fragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Password:"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView_3_account_fragment"
                app:layout_constraintVertical_bias="0.04000002" />

            <TextView
                android:id="@+id/textView_2_value_account_fragment"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginStart="10dp"
                android:gravity="center"
                android:text="000001"
                android:textSize="26dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView_2_account_fragment"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textView_2_account_fragment"
                app:layout_constraintTop_toTopOf="@+id/textView_2_account_fragment" />

            <TextView
                android:id="@+id/textView_4_account_fragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Name:"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView_2_account_fragment"
                app:layout_constraintVertical_bias="0.04000002" />

            <TextView
                android:id="@+id/textView_3_value_account_fragment"
                android:layout_width="230dp"
                android:layout_height="70dp"
                android:layout_marginStart="10dp"
                android:autoSizeTextType="uniform"
                android:autoText="true"
                android:text="herdefe404000000000000000000000000000000000000@gmail.com"
                android:textSize="26dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView_3_account_fragment"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintStart_toEndOf="@+id/textView_3_account_fragment"
                app:layout_constraintTop_toTopOf="@+id/textView_3_account_fragment" />

            <TextView
                android:id="@+id/textView_3_account_fragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Email:"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView_4_account_fragment"
                app:layout_constraintVertical_bias="0.04000002" />

            <TextView
                android:id="@+id/textView_4_value_account_fragment"
                android:layout_width="230dp"
                android:layout_height="70dp"
                android:autoSizeTextType="uniform"
                android:autoText="true"
                android:text="herdefe404000000000000000000000000000000000000@gmail.com"
                android:textSize="26dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView_4_account_fragment"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView_4_account_fragment"
                app:layout_constraintTop_toTopOf="@+id/textView_4_account_fragment" />

            <TextView
                android:id="@+id/textView_2_account_fragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Id:"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.04000002" />

            <TextView
                android:id="@+id/textView_5_value_account_fragment"
                android:layout_width="170dp"
                android:layout_height="40dp"
                android:layout_marginStart="10dp"
                android:autoSizeTextType="uniform"
                android:autoText="true"
                android:gravity="center"
                android:text="qwerty777QQQ"
                android:textSize="26dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView_5_account_fragment"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textView_5_account_fragment"
                app:layout_constraintTop_toTopOf="@+id/textView_5_account_fragment" />

            <Button
                android:id="@+id/button_log_out_account_fragment4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Log out"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView_5_account_fragment"
                app:layout_constraintVertical_bias="1.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>